  },
  "devDependencies": {
    "@types/node": "^20.12.5",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "ava": "^6.1.2",
    "eslint": "^8.57.0",
    "pkgroll": "^2.0.2",
    "tsx": "^4.7.2",
    "typescript-eslint": "^7.5.0"
  },
  "dependencies": {
    "cfb": "^1.2.2",
    "commander": "^12.0.0",
    "docx": "^8.5.0",
    "openpgp": "^5.11.1",
    "pdf-lib": "^1.17.1",
    "pkijs": "^3.0.16",
    "typescript": "^5.4.4"
  }
