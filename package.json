  "author": {
    "email": "piquark6046@proton.me",
    "name": "PiQuark6046",
    "url": "https://github.com/piquark6046"
  },
  "ava": {
    "files": [
      "tests/**/*.test.ts"
    ],
    "extensions": {
      "ts": "module"
    },
    "workerThreads": false
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
